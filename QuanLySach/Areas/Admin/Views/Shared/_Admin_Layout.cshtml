<!DOCTYPE html>

<html lang="en">
<head>
    <base href="./">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <link href="ckeditor1/ckeditor.css" rel="stylesheet" />
    <!-- Vendors styles-->
    <link rel="stylesheet" href="~/Admincess/vendors/simplebar/css/simplebar.css">
    <link rel="stylesheet" href="~/Admincess/vendors/simplebar.css">
    <!-- Main styles for this application-->
    <link href="~/Admincess/css/style.css" rel="stylesheet">
    <!-- We use those styles to show code examples, you should remove them in your application.-->
    <link href="~/Admincess/css/examples.css" rel="stylesheet">
    <link href="~/Admincess/vendors/coreui/chartjs/css/coreui-chartjs.css" rel="stylesheet">
</head>
<body>
    @await Html.PartialAsync("_Sidebar.cshtml")
    <div class="wrapper d-flex flex-column min-vh-100 bg-light">
     @await Html.PartialAsync("_Header.cshtml")
        <div class="body flex-grow-1 px-3">
           @RenderBody()
        </div>
     @await Html.PartialAsync("_Footer.cshtml")
    </div>
    <!-- CoreUI and necessary plugins-->
    <script src="~/Admincess/vendors/coreui/coreui/js/coreui.bundle.min.js"></script>
    <script src="~/Admincess/vendors/simplebar/js/simplebar.min.js"></script>
    <!-- Plugins and scripts required by this view-->
    <script src="~/Admincess/vendors/chart.js/js/chart.min.js"></script>
    <script src="~/Admincess/vendors/coreui/chartjs/js/coreui-chartjs.js"></script>
    <script src="~/Admincess/vendors/coreui/utils/js/coreui-utils.js"></script>
    <script src="js/main.js"></script>

    <script src="~/ckeditor1/ckeditor.js"></script>
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts",required:false)
</body>
</html>